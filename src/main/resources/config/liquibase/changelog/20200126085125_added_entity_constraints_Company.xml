<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Company.
    -->
    <changeSet id="20200126085125-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="license_type_id"
                                 baseTableName="company"
                                 constraintName="fk_company_license_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="license_type"/>

        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="company_configitems"
                                 constraintName="fk_company_configitems_company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>

        <addForeignKeyConstraint baseColumnNames="configitems_id"
                                 baseTableName="company_configitems"
                                 constraintName="fk_company_configitems_configitems_id"
                                 referencedColumnNames="id"
                                 referencedTableName="configuration_items"/>
        
    </changeSet>
</databaseChangeLog>
